program stupid;
    const
        a = 332;
    var
        v, w : integer;
begin
    v := 2 * (a + 1)  + 1;
    w := v div 2;

    if w > v then
        print(0);

    if v > w then
        print (1);

    if v mod 2 = 0 then
        print(111)
    else
        print(222);

    if w mod 2 = 1 then
        print(333)
    else
        print(444);

    print(v);
    print(w);
end.
