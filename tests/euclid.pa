program Euclid;

procedure gcd_recursive(u, v: integer);
  begin
    if u mod v <> 0 then
        gcd_recursive(v, u mod v)
    else
        print(v);
  end;

procedure gcd_iterative(u, v: integer);
    var
        t, gcd_iterative: integer;
    begin
        while v <> 0 do
        begin
            t := u;
            u := v;
            v := t mod v;
        end;
        gcd_iterative := abs(u);
        print(gcd_iterative);
    end;

begin
    gcd_iterative(10, 75);
    gcd_recursive(10, 75);
end.

